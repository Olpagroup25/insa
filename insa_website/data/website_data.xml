<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <!-- ========================================= -->
    <!-- MENUS PARA EL SITIO INSA                 -->
    <!-- Agua Pura y Segura - 40 años             -->
    <!-- ========================================= -->
    
    <!-- Menu Principal -->
    <record id="menu_insa_inicio" model="website.menu">
        <field name="name">Inicio</field>
        <field name="url">/</field>
        <field name="parent_id" ref="website.main_menu"/>
        <field name="sequence">1</field>
    </record>
    
    <record id="menu_insa_productos" model="website.menu">
        <field name="name">Productos</field>
        <field name="url">/#productos</field>
        <field name="parent_id" ref="website.main_menu"/>
        <field name="sequence">2</field>
    </record>
    
    <record id="menu_insa_precios" model="website.menu">
        <field name="name">Precios</field>
        <field name="url">/precios</field>
        <field name="parent_id" ref="website.main_menu"/>
        <field name="sequence">3</field>
    </record>
    
    <record id="menu_insa_contacto" model="website.menu">
        <field name="name">Contacto</field>
        <field name="url">/contacto</field>
        <field name="parent_id" ref="website.main_menu"/>
        <field name="sequence">4</field>
    </record>
</odoo>